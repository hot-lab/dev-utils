{
  "name": "@onelab/dev-utils",
  "version": "0.1.5-beta",
  "author": "",
  "license": "MIT",
  "private": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/esm/index.js",
      "default": "./dist/lib/index.js"
    },
    "./*": {
      "import": "./dist/esm/*.js",
      "default": "./dist/lib/*.js"
    }
  },
  "files": [
    "dist/esm/",
    "dist/lib/"
  ],
  "main": "dist/lib/index.js",
  "module": "dist/esm/index.js",
  "peerDependencies": {
    "react": ">=17"
  },
  "sideEffects": false,
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "scripts": {
    "build": "yarn build:lib && yarn build:esm",
    "build:esm": "babel src -d dist/esm --extensions '.js,.jsx,.ts,.tsx' --copy-files --delete-dir-on-start --plugins=transform-commonjs",
    "build:lib": "babel src -d dist/lib --extensions '.js,.jsx,.ts,.tsx' --copy-files --delete-dir-on-start --plugins=@babel/transform-modules-commonjs"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "babel-plugin-transform-commonjs": "^1.1.6",
    "chalk": "^5.0.1",
    "lodash": "^4.17.21"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hot-lab/dev-utils.git"
  },
  "bugs": {
    "url": "https://github.com/hot-lab/dev-utils/issues"
  },
  "homepage": "https://github.com/hot-lab/dev-utils#readme",
  "description": ""
}
